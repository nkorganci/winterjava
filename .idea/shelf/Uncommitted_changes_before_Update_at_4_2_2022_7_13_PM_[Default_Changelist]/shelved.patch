+package a02Review;
+// In case of call by value original value is not changed.
+public class L02_CallByValue {
+    public static void main(String[] args) {
+
+        A1 obj = new A1();
+        System.out.println("Before change " + obj.data);
+        obj.change(500);
+        System.out.println("After change " + obj.data);
+    }
+}
+
+class A1{
+    int data = 30;
+    void change(int data){
+        data = data + 200;// This data is coming from local variable
+        this.data= data+100;// changes will be in the local variable only
+        System.out.println(data);
+    }
+}
